<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import java.lang.String?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<StackPane fx:id="root" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
    <JFXDialog fx:id="dialog">
        <JFXDialogLayout>
            <Pane fx:id="splashLayout">
                <VBox fx:id="vBox">
                    <ImageView fx:id="splash">
                        <Image url="/images/logov.png" requestedWidth="275" requestedHeight="183" preserveRatio="false"
                               smooth="false">
                        </Image>
                    </ImageView>
                    <JFXProgressBar fx:id="loadProgress">
                    </JFXProgressBar>
                    <Label fx:id="progressText" text="#menu.title.cut">
                    </Label>
                </VBox>
            </Pane>
        </JFXDialogLayout>
    </JFXDialog>
    <JFXDrawer fx:id="drawer" direction="LEFT">

        <styleClass>
            <String fx:value="body"/>
        </styleClass>


    </JFXDrawer>
</StackPane>